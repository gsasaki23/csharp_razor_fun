<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>A Web Page</title>
</head>
<body>
    <h1>Here are some delicious foods!</h1>
    @{
        var FoodList = new List<String>
        {
            "Apple Pie",
            "Burritos",
            "Clams Casino",
            "Donuts"
        };

        foreach (string food in FoodList)
        {
            if (food.ToUpper()[0] == 'C')
            {
                <p style="color:red">@food</p>
            }
            else
            {
                <p>@food</p>
            }
        }
    }
</body>
</html>